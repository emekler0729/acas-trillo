{
    "name": "trillo",
    "version": "1.0.0",
    "description": "trillo app",
    "main": "index.js",
    "scripts": {
        "watch-sass": "sass --no-source-map --watch sass/main.scss css/style.css",
        "start": "npm run watch-sass",
        "make-sass": "sass --no-source-map sass/main.scss css/style.css",
        "prefix-css": "postcss css/style.css --use autoprefixer -o css/style.css --no-map",
        "build": "npm run make-sass & npm run prefix-css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/emekler0729/acas-trillo.git"
    },
    "author": "Eduard Mekler",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/emekler0729/acas-trillo/issues"
    },
    "homepage": "https://github.com/emekler0729/acas-trillo#readme",
    "devDependencies": {
        "autoprefixer": "^10.4.0",
        "postcss-cli": "^9.0.1",
        "sass": "^1.43.4"
    }
}
